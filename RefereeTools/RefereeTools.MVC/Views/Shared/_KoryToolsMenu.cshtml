        <script src="http://cdn.wijmo.com/wijmo/external/knockout-2.2.0.js" type="text/javascript"></script>
  
        <!--Wijmo Knockout Integration Library-->
        <script src="http://cdn.wijmo.com/interop/knockout.wijmo.3.20133.20.js" type="text/javascript"></script>

        <script type="text/javascript">
            $(document).ready(function () {
                $("#appMenu").wijmenu();
            });
        </script>

        <title>Kory's Web Tools</title>
    </head>
    <body>
        <ul class="nav" id="appMenu">
            <li><a>Home Page</a>
                <ul>
                    <li>@Html.ActionLink("Main Menu", "Index", "Home", new { Area =  ""}, new { })</li>
                    <li>@Html.ActionLink("About", "About", "Home", new { Area =  ""}, new { })</li>
                </ul>
            </li>
            <li><a>Officiating</a>
                <ul>
                    <li>@Html.ActionLink("Main Menu", "Index", "Home", new { Area = "Officiating" }, new { })
                        <ul>
                            <li>@Html.ActionLink("Age Group", "Index", "AgeGroup", new { Area = "Officiating" }, new { })</li>
                            <li>@Html.ActionLink("Arena", "Index", "Arena", new { Area = "Officiating" }, new { })</li>
                            <li>@Html.ActionLink("Officials", "Index", "Officials", new { Area = "Officiating" }, new { })</li>
                            <li>@Html.ActionLink("Game Details", "Index", "Details", new { Area = "Officiating" }, new { })</li>
                        </ul>
                    </li>
                </ul>
            </li>
            <li><a>Shooting</a>
                <ul>
                    <li>@Html.ActionLink("Main Menu", "Index", "Home", new { Area = "Shooting" }, new { })</li>
                </ul>
            </li>
            <li><a>Admin</a>
                <ul>
                    <li><a>Officiating</a>
                        <ul>
                            <li><a>Run Extract</a></li>
